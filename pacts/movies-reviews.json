{
  "consumer": {
    "name": "Movies"
  },
  "provider": {
    "name": "Reviews"
  },
  "interactions": [
    {
      "description": "a request for movie reviews summary",
      "providerState": "Has a few reviews",
      "request": {
        "method": "GET",
        "path": "/reviews",
        "query": "movieId[]=1",
        "matchingRules": {
          "$.query.movieId[]": {
            "min": 1
          },
          "$.query.movieId[][*].*": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "1",
            "total": 100,
            "averageRating": 7.5
          }
        ],
        "matchingRules": {
          "$.body[0].id": {
            "match": "type"
          },
          "$.body[0].total": {
            "match": "type"
          },
          "$.body[0].averageRating": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}