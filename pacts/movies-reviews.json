{
  "consumer": {
    "name": "Movies"
  },
  "provider": {
    "name": "Reviews"
  },
  "interactions": [
    {
      "description": "a request for all movies stats summary",
      "providerState": "Has reviews statistics for movie",
      "request": {
        "method": "GET",
        "path": "/stats"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "1",
            "totalReviews": 100,
            "averageRating": 7.5
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "type"
          },
          "$.body[*].totalReviews": {
            "match": "type"
          },
          "$.body[*].averageRating": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request for movies statistics",
      "providerState": "Has no statistics",
      "request": {
        "method": "GET",
        "path": "/stats"
      },
      "response": {
        "status": 404,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}